# Crie um arquivo .gitignore
cat > .gitignore << 'EOF'
# Ambiente virtual
__pycache__/
*.py[cod]
*$py.class
.venv/
venv/
ENV/

# Logs
logs/
*.log

# Arquivos de ambiente - IMPORTANTE para proteger credenciais
.env
.env.local
.env.development
.env.test
.env.production

# Bancos de dados locais
*.sqlite3
*.db

# Arquivos específicos do IDE
.idea/
.vscode/
*.swp
*.swo

# Arquivos de cache
.pytest_cache/
.coverage
htmlcov/
.tox/
.nox/
.hypothesis/

# Arquivos de distribuição
dist/
build/
*.egg-info/

# Outros
.DS_Store
node_modules/
__pycache__/
EOF